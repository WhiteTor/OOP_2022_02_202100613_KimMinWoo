package Korean_Wave;

import java.util.*;

public class main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("한류 문화 안내 프로그램에 오신 것을 환영합니다.");
		while (true) {
			System.out.println("원하시는 정보를 선택해주세요.");
			System.out.println("----------------------------------");
			System.out.println("1. 예시");
			System.out.println("2. 예시");
			System.out.println("3. 예시");
			System.out.println("4. 예시");
			System.out.println("5. 예시");
			System.out.println("----------------------------------");
			String subject = sc.next();
			System.out.println(subject);
		}
		sc.close();
	}
}